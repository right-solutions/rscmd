<% if @enquiries.any? %>
  // Fill the right box with first enquiry details in the list
  $('#div_enquiry_show').html("<%= escape_javascript(render(:partial=>'show')) %>");
  $('#div_enquiry_details').html("<%= escape_javascript(render(:partial=>'details')) %>");
  $('#div_enquiry_index').html("<%= escape_javascript(render(:partial=>"index")) %>");
  $('#div_enquiry_drop_down').html("<%= escape_javascript(theme_drop_down(@enquiries, :name))%>");
<% else %>
  var noResultsText = "<%= escape_javascript(theme_panel_message(I18n.translate('forms.no_results')))%>";
  $('#div_enquiry_index').html(noResultsText);
  $('#div_enquiry_show').html(noResultsText);
  $('#div_enquiry_details').html("");
<% end %>